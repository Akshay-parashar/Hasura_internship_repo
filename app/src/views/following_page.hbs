<div class="container">

  {{#if followings}}
  <div class="starter-template well wd_50">
    <h1>Followings</h1>
    <div class="row">
      {{#each followings}}
        <div class="col-lg-12 col-md-12 col-sm-12">
          <a href="/user_home/search?username={{this.following_username}}"><h3>{{this.following_username}}</h3></a>
        </div>
      {{/each}}
    <div>

  </div>
  {{/if}}

  {{#if no_followings}}
  <div class="starter-template">
    <h1 class="ab_head">You are not following anyone</h1>
  </div>
  {{/if}}

</div>
